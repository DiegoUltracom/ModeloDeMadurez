AFTER INSERT ON myapp_respuestascuatro
FOR EACH ROW
BEGIN
    INSERT INTO preguntas_final (empresa, id_registro, respuesta_1, respuesta_2, respuesta_3, respuesta_4, respuesta_5,
                                respuesta_6, respuesta_7, respuesta_8, respuesta_9, respuesta_10, respuesta_11, respuesta_12,
                                respuesta_13, respuesta_14, respuesta_15, respuesta_16, respuesta_17, respuesta_18,
                                respuesta_19, respuesta_20, respuesta_21, respuesta_22, respuesta_23, respuesta_24,
                                respuesta_25, respuesta_26, respuesta_27, respuesta_28, respuesta_29, respuesta_30,
                                respuesta_31, respuesta_32, respuesta_33, respuesta_34, respuesta_35)
    SELECT
        MAX(empresa) AS empresa,
        ROW_NUMBER() OVER () AS id_registro,
        MAX(respuesta_1) AS respuesta_1,
        MAX(respuesta_2) AS respuesta_2,
        MAX(respuesta_3) AS respuesta_3,
        MAX(respuesta_4) AS respuesta_4,
        MAX(respuesta_5) AS respuesta_5,
        MAX(respuesta_6) AS respuesta_6,
        MAX(respuesta_7) AS respuesta_7,
        MAX(respuesta_8) AS respuesta_8,
        MAX(respuesta_9) AS respuesta_9,
        MAX(respuesta_10) AS respuesta_10,
        MAX(respuesta_11) AS respuesta_11,
        MAX(respuesta_12) AS respuesta_12,
        MAX(respuesta_13) AS respuesta_13,
        MAX(respuesta_14) AS respuesta_14,
        MAX(respuesta_15) AS respuesta_15,
        MAX(respuesta_16) AS respuesta_16,
        MAX(respuesta_17) AS respuesta_17,
        MAX(respuesta_18) AS respuesta_18,
        MAX(respuesta_19) AS respuesta_19,
        MAX(respuesta_20) AS respuesta_20,
        MAX(respuesta_21) AS respuesta_21,
        MAX(respuesta_22) AS respuesta_22,
        MAX(respuesta_23) AS respuesta_23,
        MAX(respuesta_24) AS respuesta_24,
        MAX(respuesta_25) AS respuesta_25,
        MAX(respuesta_26) AS respuesta_26,
        MAX(respuesta_27) AS respuesta_27,
        MAX(respuesta_28) AS respuesta_28,
        MAX(respuesta_29) AS respuesta_29,
        MAX(respuesta_30) AS respuesta_30,
        MAX(respuesta_31) AS respuesta_31,
        MAX(respuesta_32) AS respuesta_32,
        MAX(respuesta_33) AS respuesta_33,
        MAX(respuesta_34) AS respuesta_34,
        MAX(respuesta_35) AS respuesta_35
    FROM (
        SELECT
            ROW_NUMBER() OVER () AS row_num,
            respuesta_1, respuesta_2, respuesta_3, respuesta_4, respuesta_5, respuesta_6, respuesta_7, respuesta_8, respuesta_9,
            NULL AS respuesta_10, NULL AS respuesta_11, NULL AS respuesta_12, NULL AS respuesta_13, NULL AS respuesta_14,
            NULL AS respuesta_15, NULL AS respuesta_16, NULL AS respuesta_17, NULL AS respuesta_18,
            NULL AS respuesta_19, NULL AS respuesta_20, NULL AS respuesta_21, NULL AS respuesta_22, NULL AS respuesta_23,
            NULL AS respuesta_24, NULL AS respuesta_25, NULL AS respuesta_26, NULL AS respuesta_27,
            NULL AS respuesta_28, NULL AS respuesta_29, NULL AS respuesta_30, NULL AS respuesta_31, NULL AS respuesta_32,
            NULL AS respuesta_33, NULL AS respuesta_34, NULL AS respuesta_35, NULL AS empresa
        FROM myapp_respuestas
        UNION ALL
        SELECT
            ROW_NUMBER() OVER () AS row_num,
            NULL AS respuesta_1, NULL AS respuesta_2, NULL AS respuesta_3, NULL AS respuesta_4, NULL AS respuesta_5,
            NULL AS respuesta_6, NULL AS respuesta_7, NULL AS respuesta_8, NULL AS respuesta_9,
            respuesta_10, respuesta_11, respuesta_12, respuesta_13, respuesta_14, respuesta_15, respuesta_16,
            respuesta_17, respuesta_18, NULL AS respuesta_19, NULL AS respuesta_20, NULL AS respuesta_21,
            NULL AS respuesta_22, NULL AS respuesta_23, NULL AS respuesta_24, NULL AS respuesta_25, NULL AS respuesta_26,
            NULL AS respuesta_27, NULL AS respuesta_28, NULL AS respuesta_29, NULL AS respuesta_30, NULL AS respuesta_31,
            NULL AS respuesta_32, NULL AS respuesta_33, NULL AS respuesta_34, NULL AS respuesta_35, NULL AS empresa
        FROM myapp_respuestasdos
        UNION ALL
        SELECT
            ROW_NUMBER() OVER () AS row_num,
            NULL AS respuesta_1, NULL AS respuesta_2, NULL AS respuesta_3, NULL AS respuesta_4, NULL AS respuesta_5,
            NULL AS respuesta_6, NULL AS respuesta_7, NULL AS respuesta_8, NULL AS respuesta_9,
            NULL AS respuesta_10, NULL AS respuesta_11, NULL AS respuesta_12, NULL AS respuesta_13, NULL AS respuesta_14,
            NULL AS respuesta_15, NULL AS respuesta_16, NULL AS respuesta_17, NULL AS respuesta_18,
            respuesta_19, respuesta_20, respuesta_21, respuesta_22, respuesta_23, respuesta_24, respuesta_25,
            respuesta_26, respuesta_27, NULL AS respuesta_28, NULL AS respuesta_29, NULL AS respuesta_30,
            NULL AS respuesta_31, NULL AS respuesta_32, NULL AS respuesta_33, NULL AS respuesta_34, NULL AS respuesta_35, NULL AS empresa
        FROM myapp_respuestastres
        UNION ALL
        SELECT
            ROW_NUMBER() OVER () AS row_num,
            NULL AS respuesta_1, NULL AS respuesta_2, NULL AS respuesta_3, NULL AS respuesta_4, NULL AS respuesta_5,
            NULL AS respuesta_6, NULL AS respuesta_7, NULL AS respuesta_8, NULL AS respuesta_9,
            NULL AS respuesta_10, NULL AS respuesta_11, NULL AS respuesta_12, NULL AS respuesta_13, NULL AS respuesta_14,
            NULL AS respuesta_15, NULL AS respuesta_16, NULL AS respuesta_17, NULL AS respuesta_18,
            NULL AS respuesta_19, NULL AS respuesta_20, NULL AS respuesta_21, NULL AS respuesta_22, NULL AS respuesta_23,
            NULL AS respuesta_24, NULL AS respuesta_25, NULL AS respuesta_26, NULL AS respuesta_27,
            respuesta_28, respuesta_29, respuesta_30, respuesta_31, respuesta_32, respuesta_33, respuesta_34, respuesta_35, NULL AS empresa
        FROM myapp_respuestascuatro
        UNION ALL
        SELECT
            ROW_NUMBER() OVER () AS row_num,
            NULL AS respuesta_1, NULL AS respuesta_2, NULL AS respuesta_3, NULL AS respuesta_4, NULL AS respuesta_5,
            NULL AS respuesta_6, NULL AS respuesta_7, NULL AS respuesta_8, NULL AS respuesta_9,
            NULL AS respuesta_10, NULL AS respuesta_11, NULL AS respuesta_12, NULL AS respuesta_13, NULL AS respuesta_14,
            NULL AS respuesta_15, NULL AS respuesta_16, NULL AS respuesta_17, NULL AS respuesta_18,
            NULL AS respuesta_19, NULL AS respuesta_20, NULL AS respuesta_21, NULL AS respuesta_22, NULL AS respuesta_23,
            NULL AS respuesta_24, NULL AS respuesta_25, NULL AS respuesta_26, NULL AS respuesta_27,
            NULL AS respuesta_28, NULL AS respuesta_29, NULL AS respuesta_30, NULL AS respuesta_31, NULL AS respuesta_32,
            NULL AS respuesta_33, NULL AS respuesta_34, NULL AS respuesta_35, empresa
        FROM myapp_registro
    ) AS combined_data
    WHERE row_num = (
        SELECT MAX(row_num) FROM (
            SELECT ROW_NUMBER() OVER () AS row_num FROM myapp_respuestas
            UNION ALL
            SELECT ROW_NUMBER() OVER () AS row_num FROM myapp_respuestasdos
            UNION ALL
            SELECT ROW_NUMBER() OVER () AS row_num FROM myapp_respuestastres
            UNION ALL
            SELECT ROW_NUMBER() OVER () AS row_num FROM myapp_respuestascuatro
            UNION ALL
            SELECT ROW_NUMBER() OVER () AS row_num FROM myapp_registro
        ) AS all_rows
    )
    GROUP BY row_num;
END
